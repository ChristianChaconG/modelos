[comment encoding = UTF-8 /]
[module generador1('http://www.eclipse.org/uml2/4.0.0/UML')]

[template public generate(m : Package)]
[comment @main/]
[file ('prueba.txt', false, 'UTF-8')]
[for (e : Class | m.allOwnedElements())]
	[e.name/]
	[for (a : Property | e.allAttributes())]
		[a.name/]:[a.type.name/]
	[/for]
[/for]
---------
[for (e : Enumeration | m.allOwnedElements())]
	[e.name/]
	[for (l : EnumerationLiteral | e.allOwnedElements())]
		[l.name/]
	[/for]
[/for]
[/file]
[/template]